<div class="navside-container">
    <div class="div-show-navside show">
        <a href="" id="show-navside">Navigation</a>
    </div>
    <div class="navside navside-hidden">
    {% for link in navsideLinks %}
        <a title="{{link.title}}" class="link" href="{{path(link.routeName, link.parameters)}} ">{{link.text}}</a>
    {% endfor %}
    {% if addManualLinks is defined %}
        {% for link in addManualLinks %}
             {{link|raw}}
        {% endfor %}
    {% endif %}
    
